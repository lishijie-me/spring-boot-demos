<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.me.mapper.OrderMapper">
    <select id="selectOrderAll" resultType="io.me.bean.Order">
        select * from trade_order
    </select>

    <insert id="addOrder" parameterType="io.me.bean.Order">
        INSERT INTO trade_order (user_id, user_name, phone, address, total_price)
        VALUES(#{userId}, #{userName}, #{phone}, #{address}, #{totalPrice});
    </insert>


</mapper>
